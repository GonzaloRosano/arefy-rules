---
interface Props {
    title: string;
    mode?: string;
    class?: string;
}
const { title, mode } = Astro.props
---
<header transition:name={mode} transition:animate="initial" class={`${Astro.props.class} ${mode} w-4/5 mx-auto rounded-lg text-white flex items-center justify-center text-6xl py-6 font-bold`}><h1>{title}</h1></header>

<style>
    .header{
        text-shadow: 0px 0 5px #ffe600;
    }

        
.gens > h1, .survival > h1, .prison > h1, .skyblock > h1 {
    text-shadow: 1px 0 8px #00000050;
}

